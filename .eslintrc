{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "spaced-comment": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "operator-linebreak": "off",
    "max-len": [
      "error",
      {
        "ignoreUrls": true,
        "code": 120
      }
    ],
    "indent": [1, 2]
  },
  "extends": ["eslint:recommended", "google"]
}
